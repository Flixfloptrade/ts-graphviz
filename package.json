{
  "name": "ts-graphviz-monorepo",
  "private": true,
  "author": "kamiazya <yuki@kamiazya.tech>",
  "description": "Graphviz library for TypeScript.",
  "homepage": "https://ts-graphviz.github.io/ts-graphviz/",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-graphviz/ts-graphviz.git"
  },
  "keywords": [
    "graphviz",
    "dot"
  ],
  "bugs": {
    "url": "https://github.com/ts-graphviz/ts-graphviz/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ts-graphviz"
  },
  "packageManager": "pnpm@8.15.1",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build --cache-dir=.turbo",
    "test": "turbo run test:unit test:integration --cache-dir=.turbo",
    "test:unit": "vitest run",
    "lint": "biome check --apply .",
    "doc": "turbo run codegen --cache-dir=.turbo && typedoc"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@vitest/coverage-istanbul": "^1.3.0",
    "svgo": "^3.2.0",
    "turbo": "^1.12.4",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.0"
  }
}
