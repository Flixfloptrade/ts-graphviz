{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "tsconfig.json"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "src/**",
        "tsconfig.json",
        "vite.config.ts",
        "!**/*.test.ts",
        "!**/*.spec.ts"
      ],
      "outputs": ["lib/**", "dist/**"]
    },
    "pretest": {
    },
    "test:integration": {
      "dependsOn": ["build", "^build"]
    }
  }
}
